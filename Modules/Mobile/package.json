{
    "name": "my-lib",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "build-fancy-box": "vue-cli-service build --no-clean --target lib --name fancy-box ./src/fancy-box/index.js --dest ./dist/fancy-box",
        "build-box-lazy-loading": "vue-cli-service --no-clean build --target lib --name box-lazy-loading ./src/box-lazy-loading/index.js --dest ./dist/box-lazy-loading",
        "build-box-lazy-loading-mob": "vue-cli-service --no-clean build --target lib --name box-lazy-loading-mob ./src/box-lazy-loading-mob/index.js --dest ./dist/box-lazy-loading-mob",
        "build-box-lazy-loading-mob-thread": "vue-cli-service --no-clean build --target lib --name box-lazy-loading-mob-thread ./src/box-lazy-loading-mob-thread/index.js --dest ./dist/box-lazy-loading-mob-thread",
        "build-box-lazy-loading-video": "vue-cli-service --no-clean build --target lib --name box-lazy-loading-video ./src/box-lazy-loading-video/index.js --dest ./dist/box-lazy-loading-video",
        "build-box-lazy-loading-img": "vue-cli-service --no-clean build --target lib --name box-lazy-loading-img ./src/box-lazy-loading-img/index.js --dest ./dist/box-lazy-loading-img",
        "build-box-lazy-loading-video-mob": "vue-cli-service --no-clean build --target lib --name box-lazy-loading-video-mob ./src/box-lazy-loading-video-mob/index.js --dest ./dist/box-lazy-loading-video-mob",
        "build-box-big-story": "vue-cli-service --no-clean build --target lib --name box-big-story ./src/box-big-story/index.js --dest ./dist/box-big-story",
        "build-light-box": "vue-cli-service build --no-clean --target lib --name light-box ./src/light-box/index.js --dest ./dist/light-box",
        "build-test": "vue-cli-service build --no-clean --target lib --name light-box ./src/test/main.js --dest ./dist/test",
        "profiler-server": "node node_modules/laravel-profiler-client/server/server.js http=8099 ws=1901",
        "profiler-client": "http-server node_modules/laravel-profiler-client/dist/ -o -s",
        "ps": "npm run profiler-server",
        "pc": "npm run profiler-client"
    },
    "devDependencies": {
        "@babel/core": "^7.14.3",
        "@babel/preset-env": "^7.14.4",
        "@vue/babel-preset-app": "^4.5.13",
        "@vue/cli-plugin-babel": "^4.5.13",
        "@vue/cli-plugin-eslint": "^4.4.6",
        "@vue/cli-service": "^4.5.13",
        "axios": "^0.21",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.2.2",
        "bootstrap": "^4.6.0",
        "css-loader": "^5.2.6",
        "eslint": "^7.3.1",
        "eslint-plugin-vue": "^6.2.2",
        "html-webpack-plugin": "^5.3.1",
        "jquery": "^3.6",
        "laravel-mix": "^6.0.6",
        "laravel-profiler-client": "^1.3.0",
        "lodash": "^4.17.19",
        "popper.js": "^1.16.1",
        "postcss": "^8.3.0",
        "resolve-url-loader": "^3.1.2",
        "rimraf": "^3.0.2",
        "sass": "^1.34.1",
        "sass-loader": "^10.2.0",
        "terser-webpack-plugin": "^4.2.3",
        "vue": "^2.6.14",
        "vue-loader": "^15.9.7",
        "vue-style-loader": "^4.1.3",
        "vue-template-compiler": "^2.6.14",
        "webpack-chunk-rename-plugin": "^1.0.3",
        "webpack-cli": "^4.7.2",
        "webpack-dev-server": "^3.11.2"
    },
    "dependencies": {
        "@vue/compiler-sfc": "^3.0.11",
        "clockwork-browser": "^1.0.0",
        "eslint-loader": "^4.0.2",
        "vue-router": "^3.5.1",
        "webpack": "^5.39.0",
        "webpack-bundle-analyzer": "^4.4.2"
    },
    "files": [
        "src",
        "assets",
        "dist/*.{js,css}"
    ]
}
